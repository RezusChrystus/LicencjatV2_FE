<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.licencjatv2_fe.UserWindow.UserController">
    <children>
        <Label layoutX="20.0" layoutY="20.0" text="Twoje Workspaces" />
        <TilePane fx:id="workspaceTilePane" hgap="15" layoutX="20.0" layoutY="98.0" prefColumns="5" prefHeight="587.0" prefWidth="667.0" vgap="15" />
      <Button fx:id="addWorkspaceButton" layoutX="565.0" layoutY="25.0" mnemonicParsing="false" onAction="#onAddWorkspaceButtonClick" text="Add Workspace" />
      <TextField fx:id="workspaceTagTextField" layoutX="343.0" layoutY="25.0" prefHeight="25.0" prefWidth="222.0" promptText="Workspace Tag" />
      <TextField fx:id="workspaceNameTextField" layoutX="343.0" layoutY="57.0" prefHeight="25.0" prefWidth="222.0" promptText="Workspace Name" />
      <Button fx:id="createWorkspaceButton" layoutX="565.0" layoutY="57.0" mnemonicParsing="false" onAction="#onCreateWorkspaceButtonClick" prefHeight="25.0" prefWidth="98.0" text="Create" />
      <Button fx:id="logoutButton" layoutX="20.0" layoutY="38.0" mnemonicParsing="false" onAction="#onLogoutButtonClick" text="Logout" />
    </children>
</AnchorPane>
